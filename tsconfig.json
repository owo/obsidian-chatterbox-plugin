{
	"compilerOptions": {
		"allowImportingTsExtensions": true,
		"baseUrl": ".",
		"inlineSourceMap": true,
		"inlineSources": true,
		"module": "nodenext",
		"target": "ES2018",
		"allowJs": true,
		"noEmit": true,
		"noImplicitAny": true,
		"moduleResolution": "nodenext",
		"importHelpers": true,
		"isolatedModules": true,
		"strictNullChecks": true,
		"lib": [
		"DOM",
		"ES5",
		"ES6",
		"ES7",
		"dom.iterable"
		]
	},
	"include": [
		"**/*.ts"
	]
}
